{
  "name": "bolao",
  "version": "2022.0.0",
  "jenkins": {
    "dns": {
      "host": "tippspiel",
      "domain": "easyware.io",
      "resourceGroup": "easyware",
      "cname": "obelix.easyware.io"
    },
    "traefik": {
      "resolver": "easywareresolver"
    },
    "nexus": {
      "hostname": "nexus.easyware.io",
      "hostedPort": 8083,
      "groupPort": 8082,
      "imageGroup": "easyware"
    },
    "docker": {
      "labels": "./config/dockerLabels.txt",
      "network": "easyware"
    },
    "nginx": {
      "env": {
        "api": "https://tippspiel-api.easyware.io"
      }
    },
    "sonar": {
      "host": "https://sonar.easyware.io",
      "properties": "./config/sonar.properties"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path=config/dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "sonar": "sonar-scanner -Dproject.settings=./config/sonar.properties"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.3",
    "@angular/common": "^14.0.3",
    "@angular/compiler": "^14.0.3",
    "@angular/core": "^14.0.3",
    "@angular/forms": "^14.1.3",
    "@angular/material": "^14.2.5",
    "@angular/platform-browser": "^14.0.3",
    "@angular/platform-browser-dynamic": "^14.1.3",
    "@angular/router": "^14.1.3",
    "@angular/service-worker": "^14.1.3",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@ngneat/transloco": "^4.1.1",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^3.1.2",
    "keycloak-angular": "^12.1.0",
    "keycloak-js": "^19.0.1",
    "lodash": "^4.17.21",
    "ng-zorro-antd": "^14.0.0",
    "ngx-flags": "^14.0.4",
    "ngx-markdown": "^14.0.1",
    "ngx-paypal": "^10.1.0",
    "rxjs": "^7.5.6",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular/cli": "^14.1.3",
    "@angular/compiler-cli": "^14.1.3",
    "@types/hammerjs": "^2.0.41",
    "@types/jasmine": "^4.3.0",
    "@types/lodash": "^4.14.184",
    "@types/node": "^17.0.45",
    "autoprefixer": "^10.4.8",
    "jasmine-core": "^4.3.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "postcss": "^8.4.16",
    "sonar-scanner": "^3.1.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  }
}
